{
  "name": "user",
  "http.path": "user",
  "plural": "users",
  "base": "User",
  "mongodb": {
    "collection": "users"
  },
  "mixins": {
    "TimeStamp": {
      "createdAt" : "created",
      "updatedAt" : "lastUpdated",
      "required" : false
    },
    "Ips": {
      "createdByIp" : "createdIp",
      "updatedByIp" : "lastUpdatedIp",
      "required" : false
    }
  },
  "hidden": [
    "emailVerified",
    "verificationToken",
    "accessTokens",
    "credentials",
    "challenges",
    "identities",
    "realm",
    "status"
  ],
  "properties": {
    "fullName": {
      "type": "string"
    },
    "language": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "accessTokens": {
      "type": "hasMany",
      "model": "accessToken",
      "foreignKey": "userId"
    },
    "identities": {
      "type": "hasMany",
      "model": "userIdentity",
      "foreignKey": "userId"
    },
    "credentials": {
      "type": "hasMany",
      "model": "userCredential",
      "foreignKey": "userId"
    },
    "personalAccount": {
      "type": "belongsTo",
      "model": "Account"
    }
  },
  "acls": [],
  "methods": []
}
