{
  "name": "Account",
  "plural": "accounts",
  "base": "PersistedModel",
  "http.path": "account",
  "mongodb": {
    "collection": "accounts"
  },
  "mixins": {
    "TimeStamp": {
      "createdAt": "created",
      "updatedAt": "lastUpdated",
      "required": false
    },
    "IPs": {
      "createdByIp": "createdIp",
      "updatedByIp": "lastUpdatedIp",
      "required": false
    },
    "Readonly": {
      "projects": true
    }
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true,
      "index": true
    },
    "public": {
      "type": "boolean",
      "index": true
    },
    "personal": {
      "type": "boolean"
    },
    "description": {
      "type": "string"
    },
    "url": {
      "type": "string"
    },
    "email": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "projects": {
      "type": "hasMany",
      "model": "Project"
    },
    "owners": {
      "type": "referencesMany",
      "model": "user",
      "foreignKey": "ownerIds"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$unauthenticated",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "accountOwner",
      "permission": "ALLOW",
      "property": "findById"
    }
  ],
  "methods": []
}
