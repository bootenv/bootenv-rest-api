{
  "name": "Account",
  "http.path": "account",
  "plural": "accounts",
  "base": "PersistedModel",
  "mongodb": {
    "collection": "accounts"
  },
  "mixins": {
    "TimeStamp": {
      "createdAt" : "created",
      "updatedAt" : "lastUpdated",
      "required" : false
    },
    "Ips": {
      "createdByIp" : "createdIp",
      "updatedByIp" : "lastUpdatedIp",
      "required" : false
    },
    "Readonly": {
      "projects": true
    }
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true,
      "index": true
    },
    "description": {
      "type": "string"
    },
    "public": {
      "type": "boolean",
      "index": true,
      "default": true
    }
  },
  "validations": [],
  "relations": {
    "projects": {
      "type": "hasMany",
      "model": "Project"
    },
    "owners": {
      "type": "referencesMany",
      "model": "user",
      "foreignKey": "ownerIds"
    }
  },
  "acls": [],
  "methods": []
}
