{
  "name": "Project",
  "http.path": "project",
  "plural": "projects",
  "base": "PersistedModel",
  "mongodb": {
    "collection": "projects"
  },
  "mixins": {
    "TimeStamp": {
      "createdAt" : "created",
      "updatedAt" : "lastUpdated",
      "required" : false
    },
    "IPs": {
      "createdByIp" : "createdIp",
      "updatedByIp" : "lastUpdatedIp",
      "required" : false
    },
    "Readonly": {
      "environments": true
    }
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true,
      "index": true
    },
    "description": {
      "type": "string"
    },
    "public": {
      "type": "boolean",
      "index": true,
      "default": true
    }
  },
  "validations": [],
  "relations": {
    "environments": {
      "type": "hasMany",
      "model": "Environment"
    },
    "variableDefinitions": {
      "type": "embedsMany",
      "model": "VariableDefinition",
      "property": "variables",
      "options": {
        "persistent": true,
        "autoId": true
      }
    }
  },
  "acls": [],
  "methods": []
}
