{
  "name": "Environment",
  "plural": "environments",
  "base": "PersistedModel",
  "http.path": "environment",
  "mongodb": {
    "collection": "environments"
  },
  "mixins": {
    "TimeStamp": {
      "createdAt": "created",
      "updatedAt": "lastUpdated",
      "required": false
    },
    "IPs": {
      "createdByIp": "createdIp",
      "updatedByIp": "lastUpdatedIp",
      "required": false
    }
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true,
      "index": true
    },
    "description": {
      "type": "string"
    },
    "color": {
      "type": "string"
    },
    "public": {
      "type": "boolean",
      "index": true,
      "default": true
    }
  },
  "validations": [],
  "relations": {
    "tokens": {
      "type": "hasMany",
      "model": "EnvironmentToken"
    },
    "variableValues": {
      "type": "embedsMany",
      "model": "VariableValue",
      "property": "variables",
      "options": {
        "persistent": true,
        "autoId": true
      }
    }
  },
  "acls": [],
  "methods": []
}
